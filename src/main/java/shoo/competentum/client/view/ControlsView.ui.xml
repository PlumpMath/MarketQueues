<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:s="urn:import:shoo.competentum.client.view"

        >
    <g:HorizontalPanel>

        <g:VerticalPanel>
            <s:CustomSlider ui:field="numCounters" value="4" minValue='1' maxValue="10" vertical="false" title="Число касс"/>
            <s:CustomSlider ui:field="numSteps" value="75" minValue='1' maxValue="150" vertical="false" title="Число шагов"/>

        </g:VerticalPanel>

        <g:HorizontalPanel>
            <s:CustomSlider ui:field="malePrc" value="33" minValue='0' maxValue="100" vertical="true"
                            title="Процет мужчин"/>
            <s:CustomSlider ui:field="femalePrc" value="33" minValue='0' maxValue="100" vertical="true"
                            title="Процет женщин"/>
            <s:CustomSlider ui:field="childrenPrc" value="34" disabled="true" minValue='0' maxValue="100"
                            vertical="true" title="Процет детей"/>
            <g:Button   ui:field="launchButton">Показать</g:Button>

        </g:HorizontalPanel>


    </g:HorizontalPanel>
</ui:UiBinder>