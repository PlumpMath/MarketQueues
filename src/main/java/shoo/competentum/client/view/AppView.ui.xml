<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >

    <ui:style>
        button {
            margin: 0.5em;
        }

        span {
            margin-right: 5px;
        }

        .content {
            padding: 2em 3em 0px;
        }

    </ui:style>


    <g:HTMLPanel addStyleNames="{style.content}">
        <h2>Расчет движения очереди</h2>

        <g:HTMLPanel ui:field="controlsContainer"/>
        <g:HTMLPanel ui:field="contentContainer"/>

    </g:HTMLPanel>


</ui:UiBinder>